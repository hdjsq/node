    "commander": "~2.12.2",
    "connect": "~3.6.6",
    "css-loader": "^0.28.11",
    "d3-queue": "~3.0.7",
    "dagre": "^0.8.2",
    "debug": "~3.1.0",
    "electron": "~2.0.2",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~15.0.1",
    "eslint-config-egg": "~4.2.0",
    "eslint-plugin-html": "~3.1.1",
    "eslint-plugin-jsx-a11y": "~5.1.1",
