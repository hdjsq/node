    "prop-types": "^15.6.2",
    "radixsort": "~1.0.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.2.0",
    "react-dom": "~16.2.0",
    "serve-static": "~1.13.2",
    "shelljs": "~0.7.8",
    "string-replace-loader": "~1.3.0",
    "style-loader": "^0.21.0",
